# 2) Build Stan data and fit
fit <- vrc_fit(
data = sim$df_obs,
t0 = sim$meta$t0,
mortality_covariates = ~ facility,   # example
reporting_covariates = ~ facility,   # example
algorithm = "sampling",
chains = 4,
iter = 1000,
seed = 123
)
